{
  "name": "expression-language-editor",
  "version": "0.0.4",
  "description": "Expression Language Editor",
  "main": "lib/index.js",
  "scripts": {
    "pin": "mo pin",
    "start": "mo start library react",
    "build": "mo build library react",
    "changelog": "mo changelog CHANGELOG.md && git add CHANGELOG.md",
    "lint:commit": "pure-lint mode=commit && npm run changelog",
    "lint:ci": "pure-lint mode=ci",
    "lint": "mo eslint [components,site] --fix",
    "postlint": "mo stylelint [components,site] --fix",
    "precommit": "npm run lint && npm run changelog",
    "prepare": "mo githooks pre-commit=\"npm run lint:commit\" commit-msg=\"commit-lint\"",
    "preview": "mo server port=8080 root_dir=./docs"
  },
  "author": {
    "name": "moneko",
    "url": "https://github.com/monako97",
    "email": "meow.monako@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/monako97/expression-language-editor"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@moneko/core": "4.0.2",
    "@moneko/react": "3.0.0",
    "n-code-live": "2.1.1",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "dependencies": {
    "@ant-design/icons": "5.6.1",
    "@antv/g": "6.1.28",
    "@antv/g6": "5.0.50",
    "@moneko/common": "1.11.0",
    "antd": "5.28.1",
    "neko-ui": "2.14.4",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "shared-store-object": "1.4.1"
  },
  "files": [
    "LICENSE",
    "README.md",
    "es",
    "lib",
    "types"
  ],
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.js"
    },
    "./*": [
      "./*"
    ]
  }
}
